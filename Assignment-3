class Person:
    def __init__(self,name,age):
        self.name=name
        self.age=age
    def display(self):
        print(self.name,self.age)
class student(Person):
    def __init__(self, name, age,marks):
        super().__init__(name, age)
        self.marks=marks
        print(self.marks)
    
    def stud(self):
        print("I am a student")

stud1=student("abc",18,90) 
stud1.display()
stud1.marks=95
stud1.name="xyz"
